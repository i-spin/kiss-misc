make mrproper

curl "https://raw.githubusercontent.com/archlinux/svntogit-packages/master/linux/trunk/config" > .config
make olddefconfig

sed '/<stdlib.h>/a #include <linux/stddef.h>' tools/objtool/arch/x86/decode.c > _
mv -f _ tools/objtool/arch/x86/decode.c

make
make INSTALL_MOD_STRIP=1 modules_install
make install
